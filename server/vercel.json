{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "rewrites": [{ "source": "/(.*)", "destination": "/api/temperature" }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}
